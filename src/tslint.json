{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "mtm",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "mtm",
      "kebab-case"
    ],
    "import-blacklist": [
      true,
      "ngx-mat-table-mediator"
    ]
  }
}
